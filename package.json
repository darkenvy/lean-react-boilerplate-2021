{
  "name": "react-boilerplate-2021",
  "version": "1.0.0",
  "description": "Streamlined React boilerplate using modern practices. Minimal",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --config webpack.config.js --color --progress",
    "build:dev": "cross-env NODE_ENV=development npm run clean && webpack --config webpack.config.js --color --progress",
    "dev": "cross-env npm run clean && NODE_ENV=development webpack serve --hot --config webpack.config.js",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkenvy/lean-react-boilerplate-2021.git"
  },
  "keywords": [
    "react",
    "preact",
    "boilerplate"
  ],
  "author": "Reno McKenzie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darkenvy/lean-react-boilerplate-2021/issues"
  },
  "homepage": "https://github.com/darkenvy/lean-react-boilerplate-2021#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^4.5.1",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
